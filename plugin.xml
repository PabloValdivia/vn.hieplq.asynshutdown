<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="NormalShutdownApp"
         name="NormalShutdownApp"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="vn.hieplq.asynshutdown.NormalShutdownApp">
         </run>
      </application>
   </extension>
   <extension
         id="AsynShutdownApp"
         name="AsynShutdownApp"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="vn.hieplq.asynshutdown.AsynShutdownApp">
         </run>
      </application>
   </extension>

</plugin>
